      });

      $('#dont_show').on("click", function(){
        $.cookie('popup_cookie', 'pclosed', {
          expires: 3,
          path: '/'
        });
        $('.hide-popup').trigger('click');
      });
      // console.log($.cookie('popup_cookie'));

      

    });
